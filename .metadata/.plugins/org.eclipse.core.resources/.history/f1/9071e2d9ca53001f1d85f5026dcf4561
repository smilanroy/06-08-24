package driverlog;

import java.util.Scanner;

public class LogicEmployee {

	//check whether its odd number or even number
	public static void main(String[] args) {
		
		//For Realtime
		// Create object for Scannrer Class
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the number:200");
		int no=sc.nextInt();
		if(no%2==0) {
			System.out.println("Even number="+no);
			
		}
		
		else {
			System.out.println("odd number="+no);
		}
	}
}
